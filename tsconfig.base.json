{
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "Preserve",
		"moduleDetection": "force",
		"jsx": "react-jsx",
		"allowJs": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false,
		"baseUrl": ".",
		"paths": {
			"@agi-cli/sdk": ["./packages/sdk/src"],
			"@agi-cli/sdk/*": ["./packages/sdk/src/*"],
			"@agi-cli/config": ["./packages/config/src"],
			"@agi-cli/config/*": ["./packages/config/src/*"],
			"@agi-cli/auth": ["./packages/auth/src"],
			"@agi-cli/auth/*": ["./packages/auth/src/*"],
			"@agi-cli/database": ["./packages/database/src"],
			"@agi-cli/database/*": ["./packages/database/src/*"],
			"@agi-cli/server": ["./packages/server/src"],
			"@agi-cli/server/*": ["./packages/server/src/*"],
			"@agi-cli/prompts": ["./packages/prompts/src"],
			"@agi-cli/prompts/*": ["./packages/prompts/src/*"],
			"@agi-cli/shared": ["./packages/shared/src"],
			"@agi-cli/shared/*": ["./packages/shared/src/*"],
			"@/ai/*": ["src/ai/*"],
			"@/auth/*": ["src/auth/*"],
			"@/cli/*": ["src/cli/*"],
			"@/db/*": ["src/db/*"],
			"@/config/*": ["src/config/*"],
			"@/prompts/*": ["src/prompts/*"],
			"@/providers/*": ["src/providers/*"],
			"@/runtime/*": ["src/runtime/*"],
			"@/server/*": ["src/server/*"]
		}
	}
}
