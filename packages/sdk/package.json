{
	"name": "@ottocode/sdk",
	"version": "0.1.192",
	"description": "AI agent SDK for building intelligent assistants - tree-shakable and comprehensive",
	"author": "nitishxyz",
	"license": "MIT",
	"homepage": "https://github.com/nitishxyz/otto#readme",
