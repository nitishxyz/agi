{
	"name": "@agi-cli/types",
	"version": "0.1.0",
	"description": "Shared TypeScript types for AGI CLI - zero dependencies",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./provider": {
			"import": "./src/provider.ts",
			"types": "./src/provider.ts"
		},
		"./auth": {
			"import": "./src/auth.ts",
			"types": "./src/auth.ts"
		},
		"./config": {
			"import": "./src/config.ts",
			"types": "./src/config.ts"
		}
	},
	"scripts": {
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "^5"
	}
}
