{
	"name": "agi",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"lint": "biome check",
		"cli": "bun run index.ts",
		"test": "bun test",
		"db:generate": "drizzle-kit generate --config=drizzle.config.ts",
		"db:reset": "bun run scripts/reset-db.ts",
		"catalog:update": "bun run scripts/update-catalog.ts",
		"compile": "mkdir -p dist && bun build --compile ./index.ts --outfile dist/agi",
		"compile:linux-x64": "mkdir -p dist && bun build --compile --target=bun-linux-x64 ./index.ts --outfile dist/agi-linux-x64",
		"compile:darwin-arm64": "mkdir -p dist && bun build --compile --target=bun-darwin-arm64 ./index.ts --outfile dist/agi-darwin-arm64"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "latest",
		"drizzle-kit": "^0.31.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^2.0.16",
		"@ai-sdk/google": "^2.0.14",
		"@ai-sdk/openai": "^2.0.30",
		"@clack/prompts": "^0.11.0",
		"ai": "^5.0.43",
		"drizzle-orm": "^0.44.5",
		"hono": "^4.9.7",
		"zod": "^4.1.8"
	}
}
