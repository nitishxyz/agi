{
	"name": "@agi-cli/cli",
	"version": "0.1.26",
	"description": "AI-powered development assistant CLI",
	"author": "ntishxyz",
	"license": "MIT",
	"type": "module",
	"bin": {
		"agi": "./dist/agi"
	},
	"scripts": {
		"dev": "bun run index.ts",
		"build": "bun build --compile ./index.ts --outfile dist/agi",
		"build:darwin-arm64": "mkdir -p dist && bun build --compile --target=bun-darwin-arm64 ./index.ts --outfile dist/agi-darwin-arm64",
		"build:darwin-x64": "mkdir -p dist && bun build --compile --target=bun-darwin-x64 ./index.ts --outfile dist/agi-darwin-x64",
		"build:linux-x64": "mkdir -p dist && bun build --compile --target=bun-linux-x64 ./index.ts --outfile dist/agi-linux-x64",
		"build:linux-arm64": "mkdir -p dist && bun build --compile --target=bun-linux-arm64 ./index.ts --outfile dist/agi-linux-arm64",
		"build:windows-x64": "mkdir -p dist && bun build --compile --target=bun-windows-x64 ./index.ts --outfile dist/agi-windows-x64.exe"
	},
	"dependencies": {
		"@agi-cli/sdk": "workspace:*",
		"@agi-cli/config": "workspace:*",
		"@agi-cli/auth": "workspace:*",
		"@agi-cli/database": "workspace:*",
		"@agi-cli/server": "workspace:*",
		"@agi-cli/prompts": "workspace:*",
		"@agi-cli/providers": "workspace:*",
		"@clack/prompts": "^0.11.0",
		"marked": "^16.3.0",
		"marked-terminal": "^7.3.0",
		"diff": "^8.0.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/marked-terminal": "^6.1.1",
		"typescript": "^5"
	}
}
