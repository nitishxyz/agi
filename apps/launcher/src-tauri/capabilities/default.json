{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Default capabilities for otto launcher",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"core:window:default",
		"core:window:allow-start-dragging",
		"core:window:allow-is-fullscreen",
		"core:webview:default",
		"opener:default",
		"dialog:default",
		"fs:default",
		"fs:allow-read-text-file",
		"shell:default",
		"process:default",
		"updater:default"
	]
}
