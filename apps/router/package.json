		"ai": "^6.0.0",
		"bs58": "^6.0.0",
		"drizzle-orm": "^0.44.5",
		"hono": "^4.9.7",
		"sst": "3.17.13",
		"tweetnacl": "^1.0.3",
		"x402": "^1.1.0"
	},
	"devDependencies": {
		"@types/bun": "latest",
